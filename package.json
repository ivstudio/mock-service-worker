{
    "name": "react-typescript-tailwindcss-webpack5-starter",
    "version": "1.0.1",
    "private": true,
    "description": "React 18, TypeScript, Tailwind CSS, and Webpack 5 starter template.",
    "keywords": [
        "react 18",
        "typescript",
        "tailwindcss",
        "webpack 5",
        "starter"
    ],
    "license": "MIT",
    "type": "module",
    "homepage": "https://ivstudio.github.io/mock-server-worker",
    "author": {
        "name": "Ignacio Villamar",
        "email": "iggy@ivstudio.com",
        "url": "https://github.com/ivstudio"
    },
    "scripts": {
        "dev": "webpack serve --config webpack/webpack.dev.ts",
        "test": "jest",
        "analyze": "ANALYZE=true npm run build",
        "build": "NODE_OPTIONS='--loader ts-node/esm' npx webpack --config webpack/webpack.prod.ts",
        "coverage": "jest --coverage",
        "format": "prettier --write src",
        "lint": "eslint ./src",
        "lint:debug": "eslint ./src --debug",
        "lint:fix": "eslint ./src --fix",
        "prepare": "husky",
        "dev:mock": "MOCK_SERVICE_WORKER=true npm run dev",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "react": "^19.1.0",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.9",
        "@babel/plugin-transform-runtime": "^7.27.4",
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.27.0",
        "@eslint/js": "^9.31.0",
        "@fullhuman/postcss-purgecss": "^7.0.2",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.9",
        "@types/webpack-bundle-analyzer": "^4.7.0",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^10.0.0",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^11.1.0",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "eslint": "^9.32.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-tailwindcss": "^3.18.2",
        "file-loader": "^6.2.0",
        "globals": "^16.2.0",
        "html-webpack-plugin": "^5.6.3",
        "husky": "^9.1.7",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "jest-fixed-jsdom": "^0.0.9",
        "mini-css-extract-plugin": "^2.9.2",
        "msw": "^2.10.4",
        "postcss": "^8.5.6",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^3.4.17",
        "ts-jest": "^29.4.1",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.40.0",
        "webpack": "^5.100.2",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-merge": "^6.0.1"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
